<http://purl.org/net/def/metashare#Actor>
      a       owl:Class ;
      rdfs:comment "Used to bring persons and organizations (in whatever role they may have with regard to the resource, e.g., resource creator, IPR holder, etc.)"@en ;
      rdfs:label "actor"@en , "actor"@es ;
      owl:equivalentClass <http://xmlns.com/foaf/0.1/Actor> .

<http://purl.org/net/def/metashare#CorpusVideoInfo>
      a       owl:Class ;
      rdfs:comment "Groups together information on the video component of a corpus."@en , "información de los componentes audivisuales del corpus"@es ;
      rdfs:label "Corpus video info"@en , "información de los vídeos del corpus"@es .

<http://purl.org/net/def/metashare#textGenre>
      a       owl:DatatypeProperty ;
      rdfs:subPropertyOf <http://purl.org/net/def/metashare#genre> .

<http://purl.org/net/def/metashare#LexicalConceptualResource>
      a       owl:Class ;
      rdfs:subClassOf <http://purl.org/net/def/metashare#LanguageResource> ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:onProperty <http://purl.org/net/def/metashare#lexicalConceptualResourceInfo> ;
                owl:someValuesFrom <http://purl.org/net/def/metashare#LexicalConceptualResourceInfo>
              ] .

<http://purl.org/net/def/metashare#ToolService>
      a       owl:Class ;
      rdfs:subClassOf <http://purl.org/net/def/metashare#LanguageResource> .

<http://purl.org/net/def/metashare#externalRef>
      a       owl:DatatypeProperty ;
      rdfs:domain <http://purl.org/net/def/metashare#LexicalConceptualResourceInfo> .

<http://purl.org/net/def/metashare#SizeUnit>
      a       owl:Class ;
      rdfs:label "unidad de medida del tamaño"@es .

<http://purl.org/net/def/metashare#theoreticModel>
      a       owl:DatatypeProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (<http://purl.org/net/def/metashare#AnnotationInfo> <http://purl.org/net/def/metashare#LanguageDescriptionInfo> <http://purl.org/net/def/metashare#LexicalConceptualResourceInfo>)
              ] .
